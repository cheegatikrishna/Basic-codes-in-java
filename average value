import java.util.*;
import java.io.*;
import java.math.*;
class Solution {
    public static void main(String args[]) {
        Scanner in = new Scanner(System.in);
        int N = in.nextInt();
        int avg=0;
        for (int i = 0; i < N; i++)
         {
            int X = in.nextInt();
            avg=avg+X;
         }
        System.out.println(avg/N);
    }
}
