import java.util.*;
import java.io.*;
import java.math.*;
class Solution {
    public static void main(String args[]) {
        Scanner in = new Scanner(System.in);
        int N = in.nextInt();
        for (int i = 0; i < N; i++) {
            int X = in.nextInt();
            if(X%2==0)
            {
                System.out.println("true");
            }else
            { 
        System.out.println("false");
            }
        }
    }
}
